<% if users.present? %>
  <table style="width:100%">
      <tr>
        <th>UserName</th>
        <th>Email</th>
        <th>Phone</th>
        <th>StreetNo</th>
        <th>Town</th>
        <th>Landmark</th>
        <th>Area</th>
        <th>Country</th>
        <th>PinCode</th>
        <th>Edit</th>
        <th>Destroy</th>
      </tr>
      <% users.each do |user| %>
        <tr>
          <td><%= link_to user.user_name, user %></td>
          <td><%= user.email %></td>
          <td><%= user.phone %></td>
            <% user.addresses.each do |address|%>
              <td><%= address.street_number%></td>
              <td><%= address.locality%></td>
              <td><%= address.route%></td>
              <td><%= address.administrative_area_level_1%></td>
              <td><%= address.country%></td>
              <td><%= address.postal_code%></td>
            <%end%>
          <td><%= link_to "Edit", edit_user_path(user) %></td>
          <td><%= link_to "Destroy", user_path(user), method: :delete,data: { confirm: "Are you sure?" } %></td>
        </tr>
      <%end%>
  </table>
<% else %>
  <h4> NO RESULT </h4>  
<%end%>  