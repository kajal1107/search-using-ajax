<div class="nested-fields">
   <p><strong>Addresses:</strong></p>
   
   <div class="right">
    <%= link_to_remove_association f, class: 'icon1' do %><i class="fa fa-minus-circle"></i><%end%>
   </div>

  <div class="form-group">
    <%= f.label :address, class: "col-md-4 control-label" %>
    <div class="col-md-8">
      <%=f.text_field :address, id: :autocomplete_address, class: "pac-target-input", placeholder: "Enter a location", autocomplete: "off"%>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :Channel_number, class: "col-md-4 control-label" %>
    <div class="col-md-8">
      <%=f.number_field :street_number, id: :street_number %>    
    </div>
  </div>

  <div class="form-group">
    <%= f.label :Town, class: "col-md-4 control-label" %>
    <div class="col-md-8">
      <%= f.text_field :locality, id: :locality %>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :Name_of_the_channel, class: "col-md-4 control-label" %>
    <div class="col-md-8">
      <%= f.text_field :route, id: :route %>  
    </div>
  </div>

  <div class="form-group">
    <%= f.label :Area, class: "col-md-4 control-label" %>
    <div class="col-md-8">
      <%= f.text_field :administrative_area_level_1, id: :administrative_area_level_1 %>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :Country, class: "col-md-4 control-label" %>
    <div class="col-md-8">
      <%= f.text_field :country, id: :country %>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :Postal_code, class: "col-md-4 control-label" %>
    <div class="col-md-8">
      <%= f.number_field :postal_code, id: :postal_code %>
    </div>
  </div>
  <%#= f.check_box :_destroy %>
  <%#= link_to_remove_association  "remove task", f %> 
</div>
